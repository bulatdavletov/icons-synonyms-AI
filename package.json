{
  "name": "icon-synonym-generator",
  "version": "1.0.0",
  "description": "AI-powered tool to generate relevant synonyms for icon components in Figma",
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch",
    "dev": "build-figma-plugin --typecheck --watch"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.0-alpha.0",
    "@create-figma-plugin/utilities": "^4.0.0-alpha.0",
    "preact": "^10.26.4"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.0-alpha.0",
    "@create-figma-plugin/tsconfig": "^4.0.0-alpha.0",
    "@figma/plugin-typings": "*",
    "@types/node": "^22.13.10",
    "esbuild": "^0.25.3",
    "typescript": "^5.3.2"
  },
  "figma-plugin": {
    "id": "1481361200325156818",
    "name": "Icon Synonym Generator",
    "main": "src/main.ts",
    "ui": "src/ui.tsx",
    "editorType": [
      "figma"
    ],
    "networkAccess": {
      "allowedDomains": [
        "https://api.openai.com"
      ]
    },
    "documentAccess": "dynamic-page"
  }
}
